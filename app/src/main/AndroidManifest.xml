<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.stablekernel.example"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".movetasktoback.ColdStartActivity"
            android:theme="@style/ColdStartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".movetasktoback.MainActivity"/>
        <activity android:name=".movetasktoback.InterimActivity"/>
    </application>

</manifest>